  },
  "dependencies": {
    "@toast-ui/vue-editor": "^3.1.1",
    "animate.css": "^4.1.1",
    "core-js": "^3.6.5",
    "markdown-it": "12.2.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@quasar/extras": "^1.10.11",
    "quasar": "^1.0.0",
    "@paciolan/remote-module-loader": "^2.6.2",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "firebase": "^7.14.2",
    "serve": "12.0.1",
    "vue-cli-plugin-quasar": "~3.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuefire": "^2.2.5",
    "sass-loader": "^8.0.2",
